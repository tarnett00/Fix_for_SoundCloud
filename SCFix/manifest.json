{
  "name": "Fix for SoundCloud","manifest_version":2,
  "version": "2.2",
  "description": "This extension will fix seeking / skipping issues in SoundCloud",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "SCFix"
  },
  "background_page": "loader.html",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ]
}